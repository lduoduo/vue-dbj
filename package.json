{
  "name": "hello-dodo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "lint": "eslint './**/*.{ts,tsx}'",
    "serve": "vue-cli-service serve",
    "build-old": "vue-cli-service build",
    "build": "rm -rf dist && npm run build:server && npm run build:client",
    "build:client-a": "cross-env NODE_ENV=production webpack --config webpack/client.config.js --json stats.json --progress --hide-modules --analyze",
    "build:client": "cross-env NODE_ENV=production webpack --config webpack/client.config.js --progress",
    "build:server": "cross-env NODE_ENV=production webpack --config webpack/server.config.js --progress",
    "start:app": "cd server && node app.js",
    "start:app-bk": "cd ssr && node app.ts",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "json-bigint": "^1.0.0",
    "koa-body": "^4.2.0",
    "koa-compress": "^5.0.1",
    "koa2": "^2.0.0-alpha.7",
    "memory-fs": "^0.5.0",
    "mobx": "^5.15.4",
    "mobx-vue": "^2.0.10",
    "vant": "^2.9.3",
    "vue": "2.6.12",
    "vue-class-component": "^7.2.3",
    "vue-property-decorator": "^8.4.2",
    "vue-router": "^3.2.0",
    "vue-server-renderer": "^2.6.12",
    "vuex": "^3.4.0",
    "vuex-module-decorators": "^0.17.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.12.1",
    "@babel/plugin-proposal-decorators": "^7.12.1",
    "@babel/plugin-proposal-object-rest-spread": "^7.12.1",
    "@babel/preset-typescript": "^7.12.7",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "@vue/cli-plugin-babel": "~4.4.0",
    "@vue/cli-plugin-eslint": "~4.4.0",
    "@vue/cli-plugin-router": "~4.4.0",
    "@vue/cli-plugin-typescript": "~4.4.0",
    "@vue/cli-plugin-vuex": "~4.4.0",
    "@vue/cli-service": "~4.4.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^5.0.2",
    "babel-loader": "^8.2.2",
    "babel-plugin-import": "^1.13.3",
    "cross-env": "^7.0.2",
    "css-loader": "^5.0.1",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-vue": "^6.2.2",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "postcss-loader": "^4.1.0",
    "postcss-pxtorem": "^5.1.1",
    "prettier": "^1.19.1",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "url-loader": "^4.1.1",
    "vue-loader": "^15.9.5",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "2.6.12",
    "webpack-cli": "^4.2.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-merge": "^5.4.0",
    "webpack-node-externals": "^2.5.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "./**/*.{js,ts,json,css,scss,md}": [
      "prettier --write",
      "yarn lint",
      "git add"
    ]
  }
}
